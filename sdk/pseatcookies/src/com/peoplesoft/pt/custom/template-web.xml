<!-- PSEatCookies - begin -->
<filter>
	<filter-name>PSEatCookies</filter-name>
	<filter-class>com.peoplesoft.pt.custom.HCEatCookiesFilter</filter-class>
	<init-param>
		<param-name>verbose</param-name>
		<param-value>true</param-value>
	</init-param>
	<init-param>
		<param-name>cookieToEat</param-name>
		<param-value>PS_TOKEN</param-value>
	</init-param>
</filter>
<filter-mapping>
	<filter-name>PSEatCookies</filter-name>
	<url-pattern>/*</url-pattern>
	<dispatcher>REQUEST</dispatcher>
	<dispatcher>FORWARD</dispatcher>
	<dispatcher>INCLUDE</dispatcher>
	<dispatcher>ERROR</dispatcher>
</filter-mapping>	
<!-- PSEatCookies - end --> 